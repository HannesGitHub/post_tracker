<!DOCTYPE html>
<html>
  <head>
    <title>PostTracker</title>
    <%= csrf_meta_tags %>

    <!--Render all the references that the admin template makes use of-->
    <!--Here is a link to the admin template that is being used in this project:
    http://www.free-css.com/free-css-templates/page210/adminlte -->
    <%= render partial: 'layouts/admin_template_references' %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body class="skin-blue">

  <!--Render layouts here-->
  <%= render partial: 'layouts/delete_confirmation_modal.html.erb' %>
  <%= render partial: 'layouts/ajax_popup_modal.html.erb' %>
  <%= render partial: 'layouts/application_body_partial.html.erb' %>
  <%= render partial: 'layouts/application_template_body_references' %>
  </body>
</html>